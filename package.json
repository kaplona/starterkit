{
  "name": "starterkit",
  "version": "0.1.0",
  "description": "node-hapi-webpack-bable-react starter kit",
  "main": "src/server.js",
  "author": "Anastasia Kaplina <nkaplina@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/kaplona/starterkit"
  },
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=production     node --harmony src/server.js",
    "build": "NODE_ENV=production     node --harmony src/scripts/make.js  && NODE_ENV=production  webpack -p --config src/config/webpack-config.js",
    "dev-start": "NODE_ENV=development    ./node_modules/nodemon/bin/nodemon.js --harmony src/server.js",
    "dev-build": "NODE_ENV=development    node --harmony src/scripts/make.js  && NODE_ENV=development  webpack -p --config src/config/webpack-config.js",
    "make": "NODE_ENV=development    node --harmony src/scripts/make.js"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "dependencies": {
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "chalk": "^1.1.3",
    "css-loader": "^0.23.1",
    "es6-promise": "^3.2.1",
    "hapi": "^14.1.0",
    "hapi-react-views": "^9.1.1",
    "inert": "^4.0.1",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "vision": "^4.1.0",
    "webpack": "^1.13.1"
  },
  "devDependencies": {
    "nodemon": "^1.10.0"
  }
}
